<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/ly_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:layout_width="250dp"
                android:layout_height="300dp"
                android:scaleType="fitXY"
                android:src="@drawable/woman" />
            <ImageView
                android:id="@+id/iv_woman_armpit"
                android:layout_marginTop="65dp"
                android:layout_marginLeft="86dp"
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:scaleType="fitXY"
                android:src="@drawable/woman_7" />
            <ImageView
                android:id="@+id/iv_woman_chest"
                android:layout_marginTop="65dp"
                android:layout_marginLeft="100dp"
                android:layout_width="50dp"
                android:layout_height="20dp"
                android:scaleType="fitXY"
                android:src="@drawable/woman_1" />
            <ImageView
                android:id="@+id/iv_woman_abdomen"
                android:layout_marginTop="90dp"
                android:layout_marginLeft="105dp"
                android:layout_width="40dp"
                android:layout_height="30dp"
                android:scaleType="fitXY"
                android:src="@drawable/woman_2" />
            <ImageView
                android:id="@+id/iv_woman_Bikini"
                android:layout_marginTop="125dp"
                android:layout_marginLeft="100dp"
                android:layout_width="50dp"
                android:layout_height="30dp"
                android:scaleType="fitXY"
                android:src="@drawable/woman_3" />
            <ImageView
                android:id="@+id/iv_woman_thigh"
                android:layout_marginTop="145dp"
                android:layout_marginLeft="132dp"
                android:layout_width="25dp"
                android:layout_height="50dp"
                android:scaleType="fitXY"
                android:src="@drawable/woman_4" />
            <ImageView
                android:id="@+id/iv_woman_knee"
                android:layout_marginTop="205dp"
                android:layout_marginLeft="95dp"
                android:layout_width="18dp"
                android:layout_height="20dp"
                android:scaleType="fitXY"
                android:src="@drawable/woman_5" />
            <ImageView
                android:id="@+id/iv_woman_leg"
                android:layout_marginTop="230dp"
                android:layout_marginLeft="140dp"
                android:layout_width="18dp"
                android:layout_height="40dp"
                android:scaleType="fitXY"
                android:src="@drawable/woman_6" />

            <ImageView
                android:id="@+id/iv_woman_arm"
                android:layout_marginTop="65dp"
                android:layout_marginLeft="155dp"
                android:layout_width="65dp"
                android:layout_height="45dp"
                android:scaleType="fitXY"
                android:src="@drawable/woman_8" />
            <ImageView
                android:id="@+id/iv_woman_hand"
                android:layout_marginTop="106dp"
                android:layout_width="28dp"
                android:layout_height="30dp"
                android:scaleType="fitXY"
                android:src="@drawable/woman_9" />

        </RelativeLayout>


    </LinearLayout>
</LinearLayout>
